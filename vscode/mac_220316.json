{
    "editor.fontFamily": "'Source Code Pro', monospace",
    "workbench.colorTheme": "GitHub Light",
    "workbench.preferredDarkColorTheme": "GitHub Dark",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.openMode": "doubleClick",
    "editor.rename.enablePreview": false,
    "window.titleBarStyle": "custom",
    "window.restoreWindows": "none",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "gitlens.views.branches.branches.layout": "list",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.autofetch": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.tree.expandMode": "doubleClick",
    "crypto-ticker.tickers": [

        {
            "symbol": "BTC",
            "currency": "USD",
            "exchange": "CCCAGG",
            "template": "{symbol} {price}"
        },
        {
            "symbol": "ETH"
        },
        {
            "symbol": "DOGE"
        }
    ],
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "liveServer.settings.donotVerifyTags": true,
    "debug.console.fontSize": 10,
    "editor.fontWeight": "600",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.minimap.maxColumn": 64,
    "editor.minimap.autohide": true,
    "workbench.preferredHighContrastColorTheme": "GitHub Light High Contrast",
    "editor.fontSize": 12.5,
    "terminal.integrated.fontFamily": "\"IBM Plex Mono\", monospace",
    "terminal.integrated.fontWeight": "500",
    "terminal.integrated.fontWeightBold": "bold"
}